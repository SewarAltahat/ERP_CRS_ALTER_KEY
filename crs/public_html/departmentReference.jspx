                                              label="#{bindings.CrsFilesLocationTlView3.hints.FileLocationDesc.label}"
                                              maximumLength="#{bindings.CrsFilesLocationTlView3.hints.FileLocationDesc.precision}"
                                              required="#{bindings.CrsFilesLocationTlView3.hints.FileLocationDesc.mandatory}"
                                              value="#{row.bindings.FileLocationDesc.inputValue}">
                                  <f:validator binding="#{row.bindings.FileLocationDesc.validator}"/>
                                </af:inputText>
                              </af:column>
                            </af:table>
                          </af:panelCollection>
                        </f:facet>
                      </af:panelStretchLayout>
                    </af:showDetailItem>
                  </af:panelTabbed>
                </af:dialog>
              </af:popup>
            </af:panelGroupLayout>
          </f:facet>
        </af:pageTemplate>
