                                </af:commandButton>
                                <af:spacer width="5"/>
                              </af:toolbar>
                            </f:facet>
                            <af:table autoHeightRows="5" blockRowNavigationOnError="always" columnStretching="last"
                                      emptyText="#{bindings.CrsFilesLocationTlView3.viewable ? menu.emptyText : menu.accessDeniedText}"
                                      fetchSize="#{bindings.CrsFilesLocationTlView3.rangeSize}"
                                      filterModel="#{bindings.CrsFilesLocationTlView3Query.queryDescriptor}"
                                      filterVisible="false" id="CrsFilesLocationTl3"
                                      queryListener="#{bindings.CrsFilesLocationTlView3Query.processQuery}"
                                      rowBandingInterval="1" rowSelection="single"
                                      rows="#{bindings.CrsFilesLocationTlView3.rangeSize}" scrollPolicy="page"
                                      selectedRowKeys="#{bindings.CrsFilesLocationTlView3.collectionModel.selectedRow}"
                                      selectionListener="#{bindings.CrsFilesLocationTlView3.collectionModel.makeCurrent}"
