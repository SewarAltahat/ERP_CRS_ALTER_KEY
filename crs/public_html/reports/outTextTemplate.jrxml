               CRS_OUT_DOC.out_DOC_YEAR out_DOCUMENT_YEAR,
               CRS_OUT_DOC.from_department_fk DEPARTMENT_CODE_FROM,
               CRS_OUT_DOC.to_department_Fk DEPARTMENT_CODE_TO,
               GET_DEPARTMENT_NAME(CRS_OUT_DOC.from_department_fk,$P{p_lang})  outDeptFromName,
               GET_DEPARTMENT_NAME(CRS_OUT_DOC.to_department_fk,$P{p_lang})  OUTDeptToName,
               (select to_char(sysdate, 'dd/MM/yyyy hh24:mi:ss') from dual) as systemDate
               FROM CRS_OUTGOING_DOCUMENTS CRS_OUT_DOC
               where CRS_OUT_DOC.OUTGOING_DOCUMENTS_PK = $P{p_outgoing_documents_pk}]]>
