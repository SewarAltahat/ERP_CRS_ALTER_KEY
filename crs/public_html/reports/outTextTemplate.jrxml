		<![CDATA[SELECT
               CRS_OUT_DOC.OUTGOING_DOCUMENTS_NO OUT_NUMBER,
               GET_LOOKUP_DESC(CRS_OUT_DOC."OUTGOING_TYPE_FK", $P{p_lang}) OUTGOING_DESC,
               CRS_OUT_DOC.OUT_DOC_DATE_G OUT_DOC_DATE_G,
               CRS_OUT_DOC.OUT_DOC_DATE_H OUT_DOC_DATE_H,
(SELECT  template_header   || ' ' ||  template_content   || ' ' ||
template_footer  FROM crs_letter_templates where letter_templates_pk  = $P{p_letter_templates_pk})OUT_TEXT ,
               GET_OUT_SUBJECT(CRS_OUT_DOC.OUTGOING_DOCUMENTS_PK,$P{p_lang})GET_OUT_SUBJECT ,
