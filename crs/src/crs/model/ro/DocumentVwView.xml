       DocumentVw.TO_DEPARTMENT_FK,                                      
       DocumentVw.PRIORITY_FK,                                      
       DocumentVw.SECURITY_LEVEL_FK,                                      
       DocumentVw.ATTACHMENTS_NO,                                      
       DocumentVw.FILES_LOCATION_FK,                                      
       DocumentVw.BARCODE,                                      
       DocumentVw.RESPOND_PERIOD,                                      
       DocumentVw.RECEIVED_DATE_G,                                      
       DocumentVw.RECEIVED_DATE_H,                                      
       DocumentVw.OUTGOING_DOCUMENTS_FK,                                      
       DocumentVw.REPLY_FL,                                      
       DocumentVw.CREATED_BY,                                     
       GET_LOOKUP_DESC(DOC_TYPE_FK,  ?) as Display_Doc_Type_Fk,                                     
       GET_LOOKUP_DESC(INCOMING_TYPE_FK, ?) as Display_Incoming_Type_Fk,                                     
       GET_LOOKUP_DESC(PRIORITY_FK, ?) as Display_Priority_Fk,                                     
       GET_LOOKUP_DESC(SECURITY_LEVEL_FK,  ?) as Display_Security_Level_Fk,              
       GET_SUBJECT_DOC(INCOMING_DOCUMENTS_PK,DOC_TYPE_FK,?) as Display_Subject_Doc,    
       GET_FILE_LOCATION_DESC (FILES_LOCATION_FK,?) as Display_Files_Location_Fk,                                     
       GET_LOOKUP_DESC(OUTGOING_DOCUMENTS_FK, ?) as Display_Outgoing_Documents_Fk,                                     
       GET_USER_FULL_NAME(CREATED_BY, ?) as Display_Created_By,                                     
       GET_USER_FULL_NAME(MODIFIED_BY, ?) as Display_Modified_By,                                     
       GET_DEPARTMENT_NAME(FROM_DEPARTMENT_FK, ?) as Display_From_Department_Fk,                                     
       GET_DEPARTMENT_NAME(TO_DEPARTMENT_FK, ?) as Display_To_Department_Fk,                       
       GET_LOOKUP_DESC(IN_DOC_TYPE_FK, ?) as Display_In_Doc_Type_Fk,                          
       DocumentVw.CREATION_DATE,                                      
       DocumentVw.MODIFIED_BY,                                      
       DocumentVw.MODIFIED_DATE,                  
       DocumentVw.INCOMING_DOCUMENTS_NO,                  
       DocumentVw.USER_DEPARTMENT_FK                                      
FROM DOCUMENT_VW DocumentVw]]></SQLQuery>
