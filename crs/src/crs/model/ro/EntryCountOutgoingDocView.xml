  <SQLQuery><![CDATA[SELECT             
count(*) as totalOutgoing,CREATED_BY,GET_USER_FULL_NAME(CREATED_BY,?)as CreatedByDesc             
FROM CRS_OUTGOING_DOCUMENTS             
where             
            
 ( (FROM_DEPARTMENT_FK = ? or ? is null or ?=''  )            
and     (TO_DEPARTMENT_FK = ? or ? is null  or ? = '')            
and     (SECURITY_LEVEL_FK = ? or ? is null  or ? = '')            
and     (PRIORITY_FK  = ? or ? is null  or ? = '')            
and     (OUT_DOC_TYPE_FK   = ? or ? is null  or ? = '')            
and (OUT_DOC_DATE_G >= ? or ? is null)            
and (OUT_DOC_DATE_G <= ? or ? is null)            
and ( OUT_DOC_DATE_H >= ? or ? is null)            
and (OUT_DOC_DATE_H <= ? or ? is null)            
and (created_by is not null)         
and  (            
  ? = ?                           
  or (? = ? and  USER_DEPARTMENT_FK = ?)                           
  or (? = ? and (USER_DEPARTMENT_FK = ? or USER_DEPARTMENT_FK IN (SELECT HIER.CHILD_DEPARTMENT FROM HCM_DEPARTMENTS_HIERARCHY HIER WHERE HIER.PARENT_DEPARTMENT = ?)))                                
  )           
)            
 group by CREATED_BY    
 order by totalOutgoing DESC]]></SQLQuery>
